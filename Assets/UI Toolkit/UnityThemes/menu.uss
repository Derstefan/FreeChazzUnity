Button {
    flex-grow: 0;
    width: 150px;
    height: 40px;
    background-color: rgb(255, 238, 173);
    color: rgb(167, 140, 106);
    font-size: 16px;
    border-radius: 5px;
    text-align: center;
}

Button:hover {
    background-color: rgb(230, 214, 184);
}

Button:active {
    background-color: rgb(244, 229, 196);
}

Button Label {
    align-self: center;
}
